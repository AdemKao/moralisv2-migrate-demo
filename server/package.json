{
  "name": "adapt-parse-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "docker_dev": "node build/index.js",
    "start": "node build/index.js",
    "build": "tsc",
    "ndev:build": "tsc",
    "ndev:run": "nodemon src/index.js",
    "ndev": "concurrently yarn ndev:*",
    "test":"nodemon src/index.ts",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "dev:db-start": "mongodb-runner start",
    "dev:db-stop": "mongodb-runner stop",
    "gen:cloud": "ts-node ./scripts/generateCloudCode.ts",
    "tsc": "tsc"
  },
  "dependencies": {
    "@codemirror/language": "^0.20.0",
    "@moralisweb3/core": "^2.7.0",
    "@moralisweb3/parse-server": "2.7.0",
    "@types/node": "^18.7.15",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "envalid": "7.3.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.5.1",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.10.1",
    "moralis": "^2.7.0",
    "ngrok": "^4.3.3",
    "parse-dashboard": "^5.0.0",
    "parse-server": "^5.2.7"
  },
  "devDependencies": {
    "@moralisweb3/eslint-config": "^1.0.0",
    "@types/cors": "^2.8.12",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "axios": "^0.27.2",
    "eslint": "^8.9.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.26.0",
    "mongodb-runner": "^4.9.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
